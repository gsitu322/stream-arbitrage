{
  "manifest_version": 2,

  "name": "Steam Recent",
  "description": "Refreshes Steam Recent",
  "version": "2.3",

  "permissions": [
    "http://steamcommunity.com/market/"
  ],
  "content_scripts": [ {
      "matches": [ "http://steamcommunity.com/market/"],
      "js": [ "jq.js", "contextscript.js"],
      "run-at": "document_end"
  } ],
  "web_accessible_resources": ["jq.js", "steamRecent.js"]
}